#!/home/thiwanka/Documents/projects/python/Transcriber/myenv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'pytube==15.0.0','console_scripts','pytube'
__requires__ = 'pytube==15.0.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pytube==15.0.0', 'console_scripts', 'pytube')()
    )
